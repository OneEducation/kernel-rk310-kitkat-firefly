/*
 * RockChip. DisplayPort screen LP097QX1
 *
 */

/ {

       disp_timings: display-timings {
                        native-mode = <&timing0>;
                        timing0: timing0 {
               screen-type = <SCREEN_EDP>;
               out-face    = <OUT_P888>;
               clock-frequency = <205000000>;
               hactive = <1536>;
               hback-porch = <52>;
               hfront-porch = <16>;
               hsync-len = <15>;

               vactive = <2048>;
               vback-porch = <3>;
               vfront-porch = <7>;
               vsync-len = <1>;
               hsync-active = <0>;
               vsync-active = <0>;
               de-active = <0>;
               pixelclk-active = <1>;
               swap-rb = <0>;
               swap-rg = <0>;
               swap-gb = <0>;
                     	};
               };
};
